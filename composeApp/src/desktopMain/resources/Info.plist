<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- 应用基本信息 -->
    <key>CFBundleName</key>
    <string>KReader</string>
    <key>CFBundleDisplayName</key>
    <string>KReader</string>
    <key>CFBundleIdentifier</key>
    <string>com.archko.reader.viewer</string>
    <key>CFBundleVersion</key>
    <string>1.0.0</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0.0</string>
    <key>CFBundlePackageType</key>
    <string>APPL</string>
    <key>CFBundleSignature</key>
    <string>????</string>
    
    <!-- 最低系统版本 -->
    <key>LSMinimumSystemVersion</key>
    <string>10.15</string>
    

    
    <!-- 文件关联配置 -->
    <key>CFBundleDocumentTypes</key>
    <array>
        <!-- PDF 文件 -->
        <dict>
            <key>CFBundleTypeName</key>
            <string>PDF Document</string>
            <key>CFBundleTypeRole</key>
            <string>Viewer</string>
            <key>CFBundleTypeExtensions</key>
            <array>
                <string>pdf</string>
            </array>
            <key>CFBundleTypeIconFile</key>
            <string>app_icon</string>
            <key>LSHandlerRank</key>
            <string>Alternate</string>
            <key>CFBundleTypeMIMETypes</key>
            <array>
                <string>application/pdf</string>
            </array>
        </dict>
        
        <!-- EPUB 文件 -->
        <dict>
            <key>CFBundleTypeName</key>
            <string>EPUB Document</string>
            <key>CFBundleTypeRole</key>
            <string>Viewer</string>
            <key>CFBundleTypeExtensions</key>
            <array>
                <string>epub</string>
            </array>
            <key>CFBundleTypeIconFile</key>
            <string>app_icon</string>
            <key>LSHandlerRank</key>
            <string>Alternate</string>
            <key>CFBundleTypeMIMETypes</key>
            <array>
                <string>application/epub+zip</string>
            </array>
        </dict>
        
        <!-- MOBI/Kindle 文件 -->
        <dict>
            <key>CFBundleTypeName</key>
            <string>MOBI Document</string>
            <key>CFBundleTypeRole</key>
            <string>Viewer</string>
            <key>CFBundleTypeExtensions</key>
            <array>
                <string>mobi</string>
                <string>azw</string>
                <string>azw3</string>
                <string>azw4</string>
            </array>
            <key>CFBundleTypeIconFile</key>
            <string>app_icon</string>
            <key>LSHandlerRank</key>
            <string>Alternate</string>
            <key>CFBundleTypeMIMETypes</key>
            <array>
                <string>application/x-mobipocket-ebook</string>
            </array>
        </dict>
        
        <!-- 文本文件 -->
        <dict>
            <key>CFBundleTypeName</key>
            <string>Text Document</string>
            <key>CFBundleTypeRole</key>
            <string>Viewer</string>
            <key>CFBundleTypeExtensions</key>
            <array>
                <string>txt</string>
                <string>rtf</string>
            </array>
            <key>CFBundleTypeIconFile</key>
            <string>app_icon</string>
            <key>LSHandlerRank</key>
            <string>Alternate</string>
            <key>CFBundleTypeMIMETypes</key>
            <array>
                <string>text/plain</string>
                <string>text/rtf</string>
            </array>
        </dict>
        
        <!-- DjVu 文件 -->
        <dict>
            <key>CFBundleTypeName</key>
            <string>DjVu Document</string>
            <key>CFBundleTypeRole</key>
            <string>Viewer</string>
            <key>CFBundleTypeExtensions</key>
            <array>
                <string>djvu</string>
                <string>djv</string>
            </array>
            <key>CFBundleTypeIconFile</key>
            <string>app_icon</string>
            <key>LSHandlerRank</key>
            <string>Alternate</string>
            <key>CFBundleTypeMIMETypes</key>
            <array>
                <string>image/vnd.djvu</string>
            </array>
        </dict>
        
        <!-- CBZ/CBR 漫画文件 -->
        <dict>
            <key>CFBundleTypeName</key>
            <string>Comic Book Archive</string>
            <key>CFBundleTypeRole</key>
            <string>Viewer</string>
            <key>CFBundleTypeExtensions</key>
            <array>
                <string>cbz</string>
                <string>cbr</string>
                <string>cb7</string>
                <string>cbt</string>
            </array>
            <key>CFBundleTypeIconFile</key>
            <string>app_icon</string>
            <key>LSHandlerRank</key>
            <string>Alternate</string>
        </dict>
        
        <!-- TIFF 图像文件 -->
        <dict>
            <key>CFBundleTypeName</key>
            <string>TIFF Image</string>
            <key>CFBundleTypeRole</key>
            <string>Viewer</string>
            <key>CFBundleTypeExtensions</key>
            <array>
                <string>tiff</string>
                <string>tif</string>
            </array>
            <key>CFBundleTypeIconFile</key>
            <string>app_icon</string>
            <key>LSHandlerRank</key>
            <string>Alternate</string>
            <key>CFBundleTypeMIMETypes</key>
            <array>
                <string>image/tiff</string>
                <string>image/tif</string>
            </array>
        </dict>
    </array>
    
    <!-- URL Schemes (如果需要) -->
    <key>CFBundleURLTypes</key>
    <array>
        <dict>
            <key>CFBundleURLName</key>
            <string>KReader Document</string>
            <key>CFBundleURLSchemes</key>
            <array>
                <string>kreader</string>
            </array>
        </dict>
    </array>
    
    <!-- 其他配置 -->
    <key>LSApplicationCategoryType</key>
    <string>public.app-category.productivity</string>
    
    <!-- 高分辨率支持 -->
    <key>NSHighResolutionCapable</key>
    <true/>
    
    <!-- 支持暗色模式 -->
    <key>NSRequiresAquaSystemAppearance</key>
    <false/>
</dict>
</plist>